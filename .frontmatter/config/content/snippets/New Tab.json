{
  "description": "Add another tab to existing tabbed content; only valid in an existing tabs shortcode.",
  "body": [
    "{{< tab \"[[name]]\" >}}",
    "",
    "[[&selection]]",
    "",
    "{{< /tab >}}"
  ],
  "fields": [
    {
      "name": "name",
      "title": "name",
      "type": "string",
      "single": true,
      "default": ""
    },
    {
      "name": "selection",
      "title": "selection",
      "type": "string",
      "single": true,
      "default": "FM_SELECTED_TEXT"
    }
  ]
}
